{
	"manifest_version":2,

	"name": "Getting Started Example",
	"description":"This extension shows a Google Image search result for the current page",
	"version": "1.0",

	"content_scripts": [
    {
    "matches": [
        "http://*/*",
        "https://*/*"
        ],
		"css":["highlight-default.css"],
		"js":["jquery.js","highlight.js","rename.js"]
	}
	],
	"permissions": [
		"activeTab",
		"https://ajax.googleapis.com/"
	]
}
